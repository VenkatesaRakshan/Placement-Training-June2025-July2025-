import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int []a=new int[n];
        int i,count=0;
        for(i=0;i<n;i++){
            a[i]=sc.nextInt();
        }
        for(i=0;i<n-1;i++){
        if(a[i]!=a[i+1]){
            count=count+1;
        }
        }
            System.out.print("There are "+(count+1)+" distinct element in the array.");
    }
}
