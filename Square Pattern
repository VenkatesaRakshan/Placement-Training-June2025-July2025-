import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int row,star;
        for(row=1;row<=n;row++){
            for(star=1;star<=n;star++){
                System.out.print("*");
            }
            System.out.println();
        }
    }
}
